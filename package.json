{
    "scripts": {
        "dfx_install": "DFX_VERSION=0.15.1 sh -ci \"$(curl -fsSL https://sdk.dfinity.org/install.sh)\"",
        "replica_start": "dfx start --background",
        "replica_stop": "dfx stop",
        "canister_deploy_local": "dfx deploy VoteContract",
        "canister_deploy_mainnet": "dfx deploy --network ic VoteContract",
        "canister_uninstall": "dfx canister uninstall-code VoteContract",
        "canister_call_get_votes": "dfx canister call VoteContract getVotes",
        "canister_call_vote_for_candidate": "dfx canister call VoteContract voteForCandidate '(\"Achmad\")'"
    },
    "dependencies": {
        "azle": "0.19.0"
    }
}

